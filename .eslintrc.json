{
    "env": {
        "browser": true,
        "es2021": true
    },
    // "extends": "plugin:vue/vue3-essential",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        // "vue"
    ],
    "rules": {
        "eqeqeq": "warn",
        "strict": "off",
        "no-const-assign": "warn",
        "no-this-before-super": "warn",
        "no-undef": "off",
        "no-unreachable": "warn",
        "no-unused-vars": "warn",
        "constructor-super": "warn",
        "valid-typeof": "warn"
    }
}
